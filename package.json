{
  "name": "blog",
  "description": "Golang, React and all things development",
  "version": "1.46.0",
  "license": "MIT",
  "keywords": [
    "golang",
    "react",
    "blog",
    "developer-blog",
    "software-engineer-blog"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build --log-pages",
    "clean": "gatsby clean",
    "config": "git update-index --skip-worktree .ghost.json",
    "postinstall": "patch-package",
    "deploy": "gatsby build && echo 'demo.jamify.org' > public/CNAME && gh-pages -f -d public -b master -r git@github.com:styxlab/styxlab.github.io"
  },
  "dependencies": {
    "gatsby": "^2.25.2",
    "gatsby-plugin-ackee-tracker": "^2.0.1",
    "gatsby-plugin-google-analytics": "^2.4.0",
    "gatsby-plugin-netlify": "^2.4.0",
    "gatsby-plugin-offline": "^3.3.2",
    "gatsby-plugin-preact": "^4.0.13",
    "gatsby-rehype-ghost-links": "^1.6.3",
    "gatsby-rehype-inline-images": "^1.9.0",
    "gatsby-rehype-prismjs": "^1.2.3",
    "gatsby-theme-ghost-dark-mode": "^1.8.2",
    "gatsby-theme-ghost-members": "^1.5.1",
    "gatsby-theme-try-ghost": "^1.49.0",
    "gatsby-transformer-rehype": "^1.9.6",
    "preact": "^10.5.3",
    "preact-render-to-string": "^5.1.11",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-plugin-ghost": "^1.5.0",
    "eslint-plugin-react": "^7.21.5",
    "gh-pages": "^3.1.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
